   "source": [
    "import pandas as pd\n",
    "data = pd.DataFrame({\"가나다\": [10, 20, -5, 30]})\n",
    "\n",
    "s = data[\"가나다\"] # Pandas Series\n",
    "v = data[\"가나다\"].values # Numpy Array\n",
    "\n",
    "print(s)\n",
    "# 0 10\n",
    "# 1 20\n",
    "# 2 -5\n",
    "# 3 30\n",
    "\n",
    "print(v)\n",
    "# array([10, 20, -5, 30])"
   ]
  },
